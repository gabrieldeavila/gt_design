{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": ["plugin:react/recommended", "airbnb"],
  "overrides": [],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "plugins": ["react"],
  "rules": {
    "linebreak-style": ["error", "windows"],
    "react/react-in-jsx-scope": "off",
    "comma-dangle": "off",
    "no-console": "off",
    "react/forbid-prop-types": "off"
  }
}
